<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".led">

    <TextView
        android:id="@+id/led1"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:gravity="center"
        android:text="Outlet 1"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/led2"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:layout_below="@+id/led1"
        android:gravity="center"
        android:text="Outlet 2"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/led3"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:layout_below="@+id/led2"
        android:gravity="center"
        android:text="Outlet 3"
        android:textSize="40dp" />

    <TextView
        android:id="@+id/led4"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:layout_below="@+id/led3"
        android:gravity="center"
        android:text="Outlet 4"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but1on"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/led1"
        android:gravity="center"
        android:text="ON"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but2off"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_below="@id/but1off"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:layout_toRightOf="@id/but1on"
        android:text="OFF"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but1off"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/but1on"
        android:text="OFF"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but2on"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_below="@id/but1on"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:layout_toRightOf="@id/led2"
        android:gravity="center"
        android:text="ON"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but3off"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_below="@id/but2off"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:layout_toRightOf="@id/but1on"
        android:text="OFF"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but4off"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_below="@id/but3off"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:layout_toRightOf="@id/but1on"
        android:text="OFF"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but3on"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_below="@id/but2on"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:layout_toRightOf="@id/led3"
        android:gravity="center"
        android:text="ON"
        android:textSize="40dp" />

    <Button
        android:id="@+id/but4on"
        android:layout_width="130dp"
        android:layout_height="70dp"
        android:layout_below="@id/but3on"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="70dp"
        android:layout_toRightOf="@id/led4"
        android:gravity="center"
        android:text="ON"
        android:textSize="40dp" />

</RelativeLayout>